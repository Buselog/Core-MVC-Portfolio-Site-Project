@model List<CoreMvcPortfolioProject.DAL.Entities.SocialMedia>
<footer>
	<div class="row">
		<div class="col-full">

			<div class="footer-logo">
				<a class="footer-site-logo" href="#home" style="font-family:'librebaskerville-bold'; color: whitesmoke; font-size: x-large">Buselog</a>
			</div>

			<ul class="footer-social">
				@foreach(var item in Model){
					<li>
						<a href="@item.SocialMediaUrl" target="_blank">
							<i class="@item.Icon" aria-hidden="true"></i>
							<span>@item.SocialMediaName</span>
						</a>
					</li>
				}
			</ul>
		</div>
	</div>

	<div class="row footer-bottom">
		<div class="col-twelve">
			<div class="copyright">
				<span>© Copyright Buselog @DateTime.Now.Year </span>
			</div>
			<div class="go-top">
				<a class="smoothscroll" title="Back to Top" href="#top"><i class="im im-arrow-up" aria-hidden="true"></i></a>
			</div>
		</div>
	</div> 
</footer>
